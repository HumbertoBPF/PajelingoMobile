<?xml version="1.0" encoding="utf-8"?>
<resources>
    <declare-styleable name="PasswordRequirement">
        <attr name="requirementText" format="string" />
        <attr name="isChecked" format="boolean" />
    </declare-styleable>

    <declare-styleable name="SimpleListItem">
        <attr name="title" format="string" />
        <attr name="description" format="string"/>
        <attr name="icon" format="reference" />
    </declare-styleable>

    <declare-styleable name="LabeledInput">
        <attr name="label" format="string" />
        <attr name="labelBias" format="float" />
        <attr name="labelBold" format="boolean" />
        <attr name="centerText" format="boolean" />
    </declare-styleable>
</resources>