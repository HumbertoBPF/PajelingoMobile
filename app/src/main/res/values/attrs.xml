<?xml version="1.0" encoding="utf-8"?>
<resources>
    <declare-styleable name="PasswordRequirement">
        <attr name="requirementText" format="string" />
        <attr name="isChecked" format="boolean" />
    </declare-styleable>

    <declare-styleable name="SimpleListItem">
        <attr name="title" format="string" />
        <attr name="description" format="string"/>
        <attr name="icon" format="reference" />
    </declare-styleable>

    <declare-styleable name="LabeledEditText">
        <attr name="label" format="string" />
        <attr name="labelBias" format="float" />
        <attr name="labelBold" format="boolean" />
        <attr name="centerText" format="boolean" />
        <attr name="type" format="enum">
            <enum name="text" value="0"/>
            <enum name="password" value="1"/>
            <enum name="email" value="2"/>
        </attr>
    </declare-styleable>

    <declare-styleable name="LabeledSpinner">
        <attr name="spinnerLabel" format="string" />
    </declare-styleable>

    <declare-styleable name="LoadingButton">
        <attr name="loadingButtonText" format="string" />
        <attr name="loadingButtonColor" format="reference" />
        <attr name="loadingButtonIcon" format="integer" />
    </declare-styleable>
</resources>